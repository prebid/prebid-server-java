{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "33Across Adapter Params",
  "description": "A schema which validates params accepted by the 33Across adapter",
  "type": "object",
  "properties": {
    "productId": {
      "type": "string",
      "description": "Product type"
    },
    "siteId": {
      "type": "string",
      "description": "[Deprecated use zoneId instead] Site Id"
    },
    "zoneId": {
      "type": "string",
      "description": "Zone Id"
    }
  },
  "anyOf": [
    {
      "required": [
        "productId",
        "zoneId"
      ]
    },
    {
      "required": [
        "productId",
        "siteId"
      ]
    }
  ]
}
